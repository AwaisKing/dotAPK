Imports System.Linq
Imports System.IO

Module Toolkit

    Function getLargestImage() As Bitmap
        Try
            Dim root As String = My.Computer.FileSystem.SpecialDirectories.Temp & "\dotAPK_TEMP_FILES"
            Dim fileList = New DirectoryInfo(root).GetFiles("*.*", SearchOption.AllDirectories)
            Dim xFile As FileInfo = (From file In fileList Let filelength = GetFileLength(file) Where filelength > 0 Order By filelength Descending Select file).First

            Using imagePath As FileStream = xFile.Open(FileMode.Open)
                Return Image.FromStream(imagePath)
            End Using
        Catch ex As Exception
            Return DefaultIcon()
        End Try
    End Function

    Function GetFileLength(ByVal fi As System.IO.FileInfo) As Long
        Try
            Return fi.Length
        Catch ex As FileNotFoundException
            Return 0
        End Try
    End Function

    Function DefaultIcon() As System.Drawing.Image
        Dim b64 As String = "".Replace(" ", "+")
        Return System.Drawing.Image.FromStream(New System.IO.MemoryStream(Convert.FromBase64String(b64)))
    End Function

    Function FormatFileSize(ByVal path As String) As String
        Dim sizeTypes() As String = {"b", "Kb", "Mb", "Gb"}
        Dim Len As Decimal = New FileInfo(path).Length
        Dim sizeType As Integer = 0
        Do While Len > 1024
            Len = Decimal.Round(Len / 1024, 2)
            sizeType += 1
            If sizeType >= sizeTypes.Length - 1 Then Exit Do
        Loop
        Dim Resp As String = Len.ToString & " " & sizeTypes(sizeType)
        Return Resp
    End Function

    Function ReadableSize(ByVal length As Decimal) As String
        Dim sizeTypes() As String = {"b", "Kb", "Mb", "Gb"}
        Dim Len As Decimal = length
        Dim sizeType As Integer = 0
        Do While Len > 1024
            Len = Decimal.Round(Len / 1024, 2)
            sizeType += 1
            If sizeType >= sizeTypes.Length - 1 Then Exit Do
        Loop
        Dim Resp As String = Len.ToString & " " & sizeTypes(sizeType)
        Return Resp
    End Function

    Function getAndroidVersion(ByVal androidSDK As String) As String
        Dim versionName As String = ""
        Select Case Integer.Parse(androidSDK)
            Case 1 : versionName = "1.0 (Astro)"
            Case 2 : versionName = "1.1 (Bender)"
            Case 3 : versionName = "1.5 (Cupcake)"
            Case 4 : versionName = "1.6 (Donut)"
            Case 5 : versionName = "2.0 (Eclair R1)"
            Case 6 : versionName = "2.0.1 (Eclair R2)"
            Case 7 : versionName = "2.1 (Eclair R3)"
            Case 8 : versionName = "2.2 (Froyo)"
            Case 9 : versionName = "2.3 - 2.3.2 (Gingerbread R1)"
            Case 10 : versionName = "2.3.3 - 2.3.7 (Gingerbread R2)"
            Case 11 : versionName = "3.0 (Honeycomb R1)"
            Case 12 : versionName = "3.1 (Honeycomb R2)"
            Case 13 : versionName = "3.2 (Honeycomb R3)"
            Case 14 : versionName = "4.0 - 4.0.2 (Icecream Sandwich R1)"
            Case 15 : versionName = "4.0.3 - 4.0.4 (Icecream Sandwich R2)"
            Case 16 : versionName = "4.1 (Jellybean R1)"
            Case 17 : versionName = "4.2 (Jellybean R2)"
            Case 18 : versionName = "4.3 (Jellybean R3)"
            Case 19 : versionName = "4.4 - 4.4.4 (KitKat)"
            Case 20 : versionName = "4.4W (KitKat Wear)"
            Case 21 : versionName = "5.0 (Lollipop R1)"
            Case 22 : versionName = "5.1 (Lollipop R2)"
            Case 23 : versionName = "6.0 (Marshmallow)"
        End Select
        Return versionName
    End Function

    Function showDialog(ByVal items As List(Of ListViewItem))
        'MagicDialog.ListView1.LargeImageList.Images.Clear()
        MagicDialog.ListView1.Items.Clear()
        MagicDialog.ImageList1.Images.Clear()
        For i As Integer = 0 To items.Count - 1
            MagicDialog.ImageList1.Images.Add(items(i).SubItems(1).Text & items(i).SubItems(2).Text, MainForm.largeList.Images(items(i).SubItems(1).Text & items(i).SubItems(2).Text))
            'MagicDialog.ListView1.Items.Add("LOL", items(i).SubItems(1).Text & items(i).SubItems(2).Text)
            Dim item As ListViewItem = New ListViewItem(New String() {"", items(i).SubItems(1).Text, items(i).SubItems(2).Text, items(i).SubItems(7).Text, items(i).SubItems(4).Text}, items(i).SubItems(1).Text & items(i).SubItems(2).Text)
            item.Tag = items(i).Tag
            MagicDialog.ListView1.Items.Add(item)
        Next
        If items.Count >= 1 Then
            MagicDialog.lblAppDuplicates.Text = "This app has " & items.Count & " duplicate"
            If items.Count >= 2 Then MagicDialog.lblAppDuplicates.Text += "s:"
            MagicDialog.pbDupAppIcon.Image = MagicDialog.ImageList1.Images(0)
            MagicDialog.lblDupAppName.Text = MagicDialog.ListView1.Items(0).SubItems(1).Text
            Return MagicDialog.ShowDialog
        Else
            Return -1
        End If
    End Function
End Module

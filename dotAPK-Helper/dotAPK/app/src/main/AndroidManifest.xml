<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" package="awaisking.dotapk">
    <application android:fullBackupContent="true" android:allowBackup="true" android:label="dotAPK Helper" android:icon="@null"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Main" android:excludeFromRecents="true" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>
</manifest>
